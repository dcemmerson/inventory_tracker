{
  "name": "inventory_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "encoding": "^0.1.13",
    "file-loader": "^6.0.0",
    "google-auth-library": "^7.14.1",
    "googleapis": "^105.0.0",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-netlify-identity": "^0.2.5",
    "react-scripts": "3.4.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lambda-serve": "netlify-lambda serve lambda-src",
    "lambda-build": "netlify-lambda build lambda-src",
    "dev": "NODE_ENV=development concurrently \"webpack-dev-server --content-base dist/\" \"npm run lambda-serve\" \"webpack --watch\"",
    "build": "NODE_ENV=production webpack && npm run lambda-build",
    "watch": "webpack --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  }
}
